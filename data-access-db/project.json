{
  "name": "data-access-db",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "data-access-db/src",
  "projectType": "library",
  "tags": ["type:data"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/data-access-db",
        "tsConfig": "data-access-db/tsconfig.lib.json",
        "packageJson": "data-access-db/package.json",
        "main": "data-access-db/src/index.ts",
        "assets": ["data-access-db/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "data-access-db/jest.config.ts"
      }
    }
  }
}
