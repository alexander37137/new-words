{
  "name": "meduza-api-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/meduza-api-client/src",
  "projectType": "library",
  "tags": ["type:data"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/meduza-api-client",
        "tsConfig": "libs/meduza-api-client/tsconfig.lib.json",
        "packageJson": "libs/meduza-api-client/package.json",
        "main": "libs/meduza-api-client/src/index.ts",
        "assets": ["libs/meduza-api-client/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/meduza-api-client/jest.config.ts"
      }
    }
  }
}
